@model IEnumerable<Movie>

@{
    ViewData["Title"] = "List";
}

<div class="album py-5 bg-light">
    <div class="container">
        <div class="row">
            <div class="col-md-3">
                @await Component.InvokeAsync("Menu")
            </div>
            <div class="col-md-9">
                <div class="row">
                    @Html.Partial("_MovieListPartial", Model)
                </div>
            </div>
        </div>
    </div>
</div>
